onSuccess: function() {
var toplistView = new YAHOO.widget.TabView("abToplist");
var latestUrlBase = '/template/ver1-0/components/articletoplist/latest.jsp';
var mostviewedUrlBase = '/template/ver1-0/components/articletoplist/mostviewed.jsp';
toplistView.on('activeIndexChange', function(event) {
var toggle = event.newValue === 0 ? YAHOO.util.Dom.removeClass : YAHOO.util.Dom.addClass;
toggle('abToplist','abToplistViewed');
});
var radioHour = new YAHOO.util.Element('abToplistHourRadio');
var radioDay = new YAHOO.util.Element('abToplistDayRadio');
var createTab = function(config) {
if (config.tabType === 'latest') {
config.dataSrc = latestUrlBase + '?sId=' + config.sId;
} else if (config.tabType === 'toplist') {
config.dataSrc = mostviewedUrlBase + '?sId=' + config.sId;
}
var tab = new YAHOO.widget.Tab(config);
if (config.tabType === 'toplist') {
radioHour.on('click', function() {
tab.set('dataSrc', mostviewedUrlBase + '?sId=' + config.sId);
if (tab.get('contentVisible')) {
tab.set('contentVisible',true);
}
});
radioDay.on('click', function() {
tab.set('dataSrc', mostviewedUrlBase + '?range=day&sId=' + config.sId);
if (tab.get('contentVisible')) {
tab.set('contentVisible',true);
}
});
}
return tab;
};
